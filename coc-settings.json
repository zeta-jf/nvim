{
    "explorer.keyMappings.global": {
        "J": false,
        "K": false
    },
    "explorer.icon.enableNerdfont": true,
    "languageserver": {
    "haskell": {
        "command": "haskell-language-server-wrapper",
        "args": [
          "--lsp"
        ],
        "rootPatterns": [
          ".stack.yaml",
          ".hie-bios",
          "BUILD.bazel",
          "cabal.config",
          "package.yaml"
        ],
        "filetypes": [
          "hs",
          "lhs",
          "haskell"
        ]
      }
    }
}
